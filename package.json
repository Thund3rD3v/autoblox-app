{
  "name": "autoblox",
  "private": true,
  "version": "0.1.0",
  "author": "AutoBlox",
  "description": "Welcome to AutoBlox, efficient Roblox Automation solutions that takes subtlety and safety to heart. No scripts or injectors needed here, just a smooth and worry-free experience. The best part? It's completely free to get started! Head over to autoblox.xyz  ",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "electron-builder install-app-deps",
    "preview": "vite preview"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^3.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "class-variance-authority": "^0.6.1",
    "clsx": "^2.0.0",
    "electron-updater": "^6.1.1",
    "framer-motion": "^10.14.0",
    "lucide-react": "^0.260.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "recoil": "^0.7.7",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@types/node": "^20.4.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.4.0",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "repository": "https://github.com/autoblox-xyz/release",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "app.autoblox.hub",
    "productName": "AutoBlox",
    "target": "NSISS",
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": true,
      "artifactName": "${productName}-Installer.${ext}",
      "license": "License.html"
    },
    "icon": "autoblox.ico",
    "files": [
      "dist-electron",
      "dist",
      "public"
    ],
    "directories": {
      "output": "release/${version}"
    }
  },
  "main": "dist-electron/main.js"
}
